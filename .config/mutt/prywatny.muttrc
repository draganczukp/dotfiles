# vim: filetype=neomuttrc
# muttrc file for account $title
set realname = "Przemek Dragańczuk"
set from = "admin@draganczuk.me"
set sendmail = "/usr/bin/msmtp -a prywatny"
set folder = "~/.mail/prywatny"
set spoolfile = "+INBOX"
set record = "~/.mail/prywatny/Sent"

alias me "Przemek Dragańczuk" "admin@draganczuk.me"

set mbox_type = Maildir
set ssl_starttls = yes
set ssl_force_tls = yes

unmailboxes *
