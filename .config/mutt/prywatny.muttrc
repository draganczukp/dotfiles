# vim: filetype=neomuttrc
# muttrc file for account $title
set realname = "Przemek Dragańczuk"
set from = "admin@draganczuk.tk"
set sendmail = "/usr/bin/msmtp -a draganczuktk"
set folder = "~/.mail/draganczuktk"
set spoolfile = "+INBOX"
set record = "~/.mail/draganczuktk/Sent"

alias me "Przemek Dragańczuk" "mail@draganczuk.tk"

set mbox_type = Maildir
set ssl_starttls = yes
set ssl_force_tls = yes

unmailboxes *
