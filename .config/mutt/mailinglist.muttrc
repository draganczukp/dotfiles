# vim: filetype=neomuttrc
# muttrc file for account $title
set realname = "Przemek Dragańczuk"
set from = "mailing-list@draganczuk.tk"
set sendmail = "/usr/bin/msmtp -a mailinglist"
set folder = "~/.mail/mailinglist"
set spoolfile = "+INBOX"
set record = "~/.mail/mailinglist/Sent"

alias me "Przemek Dragańczuk" "mailing-list@draganczuk.tk"

set mbox_type = Maildir
set ssl_starttls = yes
set ssl_force_tls = yes

unmailboxes *
