#!/bin/zsh

[ -e ~/.screenlayout/layout.sh ] && ~/.screenlayout/layout.sh &
setxkbmap -option caps:escape -layout pl &
~/bin/wallpaper &
~/bin/start-compton &
dunst &
qutebrowser &
spotify &
/usr/lib/kdeconnectd &

~/bin/run-on-clock 1m ~/bin/checkmail &
~/bin/run-on-clock 1m ~/bin/calendar &

xset s 0
wmname LD3D
