#!/bin/zsh

[ -e ~/.screenlayout/layout.sh ] && ~/.screenlayout/layout.sh &
setxkbmap -option caps:escape -layout pl &
~/bin/wallpaper &
~/bin/start-compton &
# dunst &
qutebrowser &
spotify &
/usr/lib/kdeconnectd &

~/bin/run-on-clock 1m ~/bin/checkmail &
~/bin/run-on-clock 1m ~/bin/calendar a &
~/bin/run-on-clock 1m ~/bin/check-phone &

xset s 0
xset r rate 500 50

sleep 10s
wmname LG3D
